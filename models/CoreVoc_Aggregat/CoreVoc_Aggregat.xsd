    <xs:include schemaLocation="../CoreVoc_Common/CoreVoc_CommonTypes.xsd"/>
    <xs:import namespace="http://purl.org/dc/terms/" schemaLocation="../CoreVoc_Aggregat/dcterms.xsd" />

